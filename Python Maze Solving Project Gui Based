      "Greedy Best First Search:  ['down', 'right', 'right', 'right', 'right', 'down', 'down', 'down', 'down', 'down', 'down', 'down', 'right', 'right', 'right', 'down', 'right', 'down', 'down', 'right', 'right', 'right', 'right', 'right', 'up', 'up', 'right', 'right', 'right', 'right', 'down', 'down', 'down', 'down', 'down', 'down', 'down', 'down']\n",
      "\n",
      "Path cost:  129\n"
     ]
    }
   ],
   "source": [
    "from queue import PriorityQueue\n",
    "\n",
    "maze_instance = Maze(20, 20, color={'Goal': (0, 128, 0), 'Wall': (165,42,42), 'OurAgent': (255, 255, 0)})\n",
    "\n",
    "def is_valid_position(pos, maze):\n",
    "    x, y = pos\n",
    "    return 0 <= x < len(maze) and 0 <= y < len(maze[0]) and maze[x][y] != 1\n",
    "\n",
    "def manhattan_distance(start, goal):\n",
    "    return abs(start[0] - goal[0]) + abs(start[1] - goal[1])\n",
    "\n",
    "from queue import PriorityQueue\n",
    "\n",
    "def greedy_best_first_search(maze, start, goal):\n",
    "    queue = PriorityQueue()  \n",
    "    queue.put((0, start, [], 0))  \n",
    "    visited = set()  \n",
    "\n",
    "    while not queue.empty():\n",
    "        emp, current_pos, path, current_cost = queue.get()  \n",
    "        x, y = current_pos\n",
    "\n",
    "      \n",
    "        if current_pos == goal:\n",
    "            return path \n",
    "\n",
    "       \n",
    "        for next_x, next_y, direction, cost in [(0, 1, \"right\", 4), (0, -1, \"left\", 1), (1, 0, \"down\", 3), (-1, 0, \"up\", 2)]:\n",
    "            new_pos = (x + next_x, y + next_y)\n",
    "            valid_position = is_valid_position(new_pos, maze)\n",
    "           \n",
    "            if valid_position == 1 and new_pos not in visited:\n",
    "               \n",
    "                priority = manhattan_distance(new_pos, goal) + current_cost + cost\n",
    "                queue.put((priority, new_pos, path + [direction], current_cost + cost)) \n",
    "                visited.add(new_pos)  \n",
    "\n",
    "    return None \n",
    "\n",
    "def algo(maze):\n",
    "    \"\"\"\n",
    "    implement this function using the maze from input\n",
    "    3 in the maze represents the location of the agent\n",
    "    2 represents the goal\n",
    "    1 represents the wall\n",
    "    0 represent the free space in which our agent can walk\n",
    "    return and array of moves like this e.g. [\"down\", \"right\", \"up\", \"left\", \"left\", \"down\"]\n",
    "    so that the agent will be at the goal \n",
    "    use maze_instance.run(0) to show updates without running logic\n",
    "    \"\"\"\n",
    "   \n",
    "    start_pos = None\n",
    "    goal_pos = None\n",
    "    for i in range(len(maze)):\n",
    "        for j in range(len(maze[0])):\n",
    "            if maze[i][j] == 3:\n",
    "                start_pos = (i, j)\n",
    "            elif maze[i][j] == 2:\n",
    "                goal_pos = (i, j)\n",
    "\n",
    "    if start_pos is None or goal_pos is None:\n",
    "        print(\"Starting position or goal position not found.\")\n",
    "        return None\n",
    "\n",
    "    \n",
    "    path = greedy_best_first_search(maze, start_pos, goal_pos)\n",
    "    if path is None:\n",
    "        print(\"No path found from the starting position to the goal position.\")\n",
    "        return None\n",
    "    else:\n",
    "        print(\"Path:\", path)\n",
    "        return path\n",
    "\n",
    "\n",
    "for i in range(len(maze_layout)):\n",
    "    for j in range(len(maze_layout[0])):\n",
    "        if maze_layout[i][j] == 1:\n",
    "            maze_instance.add_thing(Wall(), [i, j])\n",
    "\n",
    "agent = OurAgent(program)\n",
    "goal = Goal()\n",
    "start = [1,1]\n",
    "end = [18,18]\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.exogenous_change()\n",
    "it = -1\n",
    "\n",
    "maze_instance.delete_thing(agent)\n",
    "maze_instance.delete_thing(goal)\n",
    "\n",
    "steps = algo(np.transpose(maze_layout))\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.run(len(steps))\n",
    "print('Greedy Best First Search: ', steps)\n",
    "print('\\nPath cost: ',(steps.count('left') * 1) + (4 * steps.count('right')) + (2 * steps.count('up')) + (3 * steps.count('down')))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from queue import PriorityQueue\n",
    "\n",
    "maze_instance = Maze(20, 20, color={'Goal': (0, 128, 0), 'Wall': (165,42,42), 'OurAgent': (255, 255, 0)})\n",
    "\n",
    "def is_valid_position(pos, maze):\n",
    "    x, y = pos\n",
    "    return 0 <= x < len(maze) and 0 <= y < len(maze[0]) and maze[x][y] != 1\n",
    "\n",
    "def manhattan_distance(start, goal):\n",
    "    return abs(start[0] - goal[0]) + abs(start[1] - goal[1])\n",
    "\n",
    "def A_star_search(maze, start, goal):\n",
    "    queue = PriorityQueue()\n",
    "    queue.put((0, start, []))\n",
    "    visited = set()\n",
    "\n",
    "    while not queue.empty():\n",
    "        current_cost, current_pos, path = queue.get()\n",
    "        x, y = current_pos\n",
    "\n",
    "        if current_pos == goal:\n",
    "            return path, current_cost\n",
    "\n",
    "        for next_x, next_y, direction, cost in [(0, 1, \"right\", 4), (0, -1, \"left\", 1), (1, 0, \"down\", 3), (-1, 0, \"up\", 2)]:\n",
    "            new_pos = (x + next_x, y + next_y)\n",
    "\n",
    "            if is_valid_position(new_pos, maze) and new_pos not in visited:\n",
    "                new_cost = current_cost + cost\n",
    "                priority = new_cost + manhattan_distance(new_pos, goal)\n",
    "                queue.put((new_cost, new_pos, path + [direction]))\n",
    "                visited.add(new_pos)\n",
    "\n",
    "    return None, None\n",
    "\n",
    "def algo(maze):\n",
    "    \"\"\"\n",
    "    implement this function using the maze from input\n",
    "    3 in the maze represents the location of the agent\n",
    "    2 represents the goal\n",
    "    1 represents the wall\n",
    "    0 represent the free space in which our agent can walk\n",
    "    return and array of moves like this e.g. [\"down\", \"right\", \"up\", \"left\", \"left\", \"down\"]\n",
    "    so that the agent will be at the goal \n",
    "    use maze_instance.run(0) to show updates without running logic\n",
    "    \"\"\"\n",
    "    start_pos = None\n",
    "    goal_pos = None\n",
    "    for i in range(len(maze)):\n",
    "        for j in range(len(maze[0])):\n",
    "            if maze[i][j] == 3:\n",
    "                start_pos = (i, j)\n",
    "            elif maze[i][j] == 2:\n",
    "                goal_pos = (i, j)\n",
    "\n",
    "    if start_pos is None or goal_pos is None:\n",
    "        print(\"Starting position or goal position not found.\")\n",
    "        return None\n",
    "\n",
    "    path = A_star_search(maze, start_pos, goal_pos)\n",
    "    return path\n",
    "\n",
    "    \n",
    "\n",
    "for i in range(len(maze_layout)):\n",
    "    for j in range(len(maze_layout[0])):\n",
    "        if maze_layout[i][j] == 1:\n",
    "            maze_instance.add_thing(Wall(), [i, j])\n",
    "\n",
    "agent = OurAgent(program)\n",
    "goal = Goal()\n",
    "start = [1,1]\n",
    "end = [18,18]\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.exogenous_change()\n",
    "it = -1\n",
    "\n",
    "maze_instance.delete_thing(agent)\n",
    "maze_instance.delete_thing(goal)\n",
    "\n",
    "steps, cost = algo(np.transpose(maze_layout))\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.run(len(steps))\n",
    "print('A* Search: ', steps)\n",
    "print(f'\\nPath cost: {cost}')"
   ]
  }
