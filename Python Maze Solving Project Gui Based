   "source": [
    "maze_instance = Maze(20, 20, color={'Goal': (0, 128, 0), 'Wall': (165,42,42), 'OurAgent': (255, 255, 0), 'Visited':(125,223,43)})\n",
    "\n",
    "def is_valid_position(pos, maze):\n",
    "    x, y = pos\n",
    "    return 0 <= x < len(maze) and 0 <= y < len(maze[0]) and maze[x][y] != 1\n",
    "\n",
    "from collections import deque\n",
    "import numpy as np\n",
    "\n",
    "def bfs(maze, start, goal):\n",
    "    queue = [(start, [])]  \n",
    "    front = 0  \n",
    "    end = 1  \n",
    "    visited = set() \n",
    "\n",
    "    while front < end:\n",
    "        current_pos, path = queue[front]   \n",
    "        front += 1  \n",
    "        x, y = current_pos\n",
    "\n",
    "        if current_pos == goal:\n",
    "            return path  \n",
    "\n",
    "        for next_x, next_y, direction in [(0, 1, \"right\"), (0, -1, \"left\"), (1, 0, \"down\"), (-1, 0, \"up\")]:\n",
    "            new_pos = (x + next_x, y + next_y) \n",
    "\n",
    "            if is_valid_position(new_pos, maze) and new_pos not in visited:\n",
    "                queue.append((new_pos, path + [direction]))\n",
    "                end += 1  \n",
    "                visited.add(new_pos)  \n",
    "\n",
    "    return None  \n",
    "\n",
    "def algo(maze):\n",
    "    \"\"\"\n",
    "    implement this function using the maze from input\n",
    "    3 in the maze represents the location of the agent\n",
    "    2 represents the goal\n",
    "    1 represents the wall\n",
    "    0 represent the free space in which our agent can walk\n",
    "    return and array of moves like this e.g. [\"down\", \"right\", \"up\", \"left\", \"left\", \"down\"]\n",
    "    so that the agent will be at the goal \n",
    "    use maze_instance.run(0) to show updates without running logic\n",
    "    \"\"\"\n",
    "    start_pos = None  \n",
    "    goal_pos = None    \n",
    "    for i in range(len(maze)):\n",
    "        for j in range(len(maze[0])):\n",
    "            if maze[i][j] == 3:\n",
    "                start_pos = (i, j)\n",
    "            elif maze[i][j] == 2:\n",
    "                goal_pos = (i, j)\n",
    "\n",
    "    if start_pos is None or goal_pos is None:\n",
    "        print(\"Starting position or goal position not found.\")\n",
    "        return None\n",
    "\n",
    "    steps = bfs(maze, start_pos, goal_pos) \n",
    "    return steps\n",
    "\n",
    "\n",
    "for i in range(len(maze_layout)):\n",
    "    for j in range(len(maze_layout[0])):\n",
    "        if maze_layout[i][j] == 1:\n",
    "            maze_instance.add_thing(Wall(), [i, j])\n",
    "\n",
    "\n",
    "agent = OurAgent(program)\n",
    "goal = Goal()\n",
    "start = [1,1]\n",
    "end = [18,18]\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.exogenous_change()\n",
    "it = -1\n",
    "\n",
    "maze_instance.delete_thing(agent)\n",
    "maze_instance.delete_thing(goal)\n",
    "\n",
    "\n",
    "steps = algo(np.transpose(maze_layout))\n",
    "maze_instance.add_thing(agent, start)\n",
    "maze_instance.add_thing(goal, end)\n",
    "maze_instance.run(len(steps))\n",
    "print('\\t\\t\\t\\t\\tGoal Reached\\n\\n')\n",
    "\n",
    "print('Steps: ',steps, end=\" \")\n",
    "print('\\nStep cost:', steps.count('right') + steps.count('left') + steps.count('up') + steps.count('down'))"
   ]
  },
